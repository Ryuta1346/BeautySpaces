<%= form_for(@menu, url: admin_stylist_menus_path) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :menu_name %>
  <%= f.text_field :menu_name %>
  <br/>
  <%= f.label :menu_price %>
  <%= f.number_field :menu_price %>
  <br/>
  <%= f.label :operation_time %>
  <%= f.number_field :operation_time %>
  <br/>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <br/>
  <%= f.label :memo %><br/>
  <%= f.text_area :memo %>
  <br/>
  <%= f.submit "編集" %>
<% end %>