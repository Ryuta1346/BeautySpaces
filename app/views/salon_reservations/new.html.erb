<h1>SalonReservations#new</h1>
<p>Find me in app/views/salon_reservations/new.html.erb</p>
<%= form_for(salon_salon_reservations_path) do |f| %>
  <%#= render 'shared/error_messages', object: f.object %>

  <div>
    <%= f.label :prefecture %>
    <%= f.collection_select :prefecture_id, Prefecture.all, :id, :name %>
  </div>
  <div>
    <%= f.label :book_time %>
    <%= f.datetime_field :book_time %>
  </div>

  <%= f.submit "Submit"%>
<% end %>